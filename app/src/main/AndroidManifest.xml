<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spacecoatl.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >
        <uses-sdk android:minSdkVersion="14"
            android:targetSdkVersion="14" />
        <activity
            android:name="com.example.spacecoatl.app.Spacecoatl"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.spacecoatl.app.Card"
            android:label="@string/title_activity_card" >
        </activity>
        <activity
            android:name="com.example.spacecoatl.app.Notification"
            android:label="@string/title_activity_notification"
            android:parentActivityName="com.example.spacecoatl.app.Card" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="Card" />
        </activity>
    </application>

</manifest>
